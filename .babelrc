{
  "presets": ["babel-preset-expo"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "assets": "./src/assets",
          "config": "./src/config",
          "components": "./src/components",
          "pages": "./src/pages",
          "locale": "./src/locale",
          "constants": "./src/constants",
          "styles": "./src/styles",
          "utils": "./src/utils",
          "reducers": "./src/reducers",
          "store": "./src/store",
          "navigation": "./src/navigation"
        },
        "extensions": [".js", ".ios.js", ".android.js"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  }
}
